@model booking_car_app.Models.BookingViewModel

@{
    ViewData["Title"] = "Booking";
}
<script>


</script>


<h1>Booking</h1>
<form  asp-action="Booking">
    <div class="form-group row">
        <label for="addressFrom" class="col-sm-2 col-form-label">From: </label>
        <div class="col-sm-10">
            <input type="addressFrom" class="form-control" id="addressFrom" onclick="getLocation()">
        </div>
    </div>
    <div class="form-group row">
        <label for="addressTo" class="col-sm-2 col-form-label">To: </label>
        <div class="col-sm-10">
            <input type="addressTo" class="form-control" id="addressTo">
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-outline-success" onclick="dogetRedirect_map()">Book</button>
        </div>
    </div>
</form>
<iframe width="600"
        height="450"
        style="border:0"
        loading="lazy"
        allowfullscreen
        referrerpolicy="no-referrer-when-downgrade"
        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBPVb8YnppKIeJg2-LydOZDVa59BnY-p9Y
    &q=Space+Needle,Seattle+WA">
</iframe>

<div>
    <a asp-action="Index">Back to Homepage</a>
</div>